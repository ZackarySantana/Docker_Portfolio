FROM node:14

WORKDIR /app

# Get dependancies
ENV PATH /app/node_modules/.bin:$PATH
COPY ./config/frontend.json ./package.json
COPY ./config/config-overrides.js ./config-overrides.js
RUN npm install --silent

# Remove generated files
#RUN rm -r public src

# Deploy
EXPOSE $PORT
CMD ["npm", "start"]
